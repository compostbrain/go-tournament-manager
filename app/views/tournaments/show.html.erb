<div class="container">
  <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s1">
          <input id="icon_prefix" type="number" class="validate" default="1">
          <label for="icon_prefix">Round #</label>
        </div>
      </div>
    </form>
  </div>
  <div class="row col s12">
    <table class="col s4 bordered">
      <thead>
        <tr>
          <th>Name</th>
          <th>Rank</th>
          <th>Club</th>
        </tr>
      </thead>
      <tbody id="active-players">
        <% @players.each do |player| %>
          <%= render partial: "players/player", locals: { player: player } %>
        <% end %>
      </tbody>

    </table>

  </div>
</div>
