<div class="row">
  <div class="col s12">
    <table class="c bordered">
      <h1>Standings</h1>
      <thead>
        <tr>
          <th>Placing</th>
          <th>Name</th>
          <th>Rank</th>
          <th></th>
        </tr>
      </thead>
      <tbody id="active-players">
        <% @players.each do |player| %>
        <%= render partial: "standings", locals: { player: player } %>
        <% end %>
      </tbody>

    </table>
  </div>
